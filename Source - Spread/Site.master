    <script>
        $(document).ready(function () {
            $("#menushow").click(function () {
                $("#closemenu").toggle('fast');
            });
            $("#closemenu").click(function () {
                $("#closemenu").hide('fast');
            });
            $("#allmenu").click(function () {
                $("#closemenu").show('fast');
            });
            $("#divimg_stfphoto").click(function () {
                $("#showstaffinfo").toggle('fast');
            });
            $("#showstaffinfo").click(function () {
                $("#showstaffinfo").hide('fast');
            });
            $("#colorpickerdiv_main").click(function () {
                $("#colorpickerdiv").toggle('fast');
            });
        });
    </script>
    <script type="text/javascript">
        function Color_Changed(sender) {
            sender.get_element().value = "#" + sender.get_selectedColor();
        }
    </script>
